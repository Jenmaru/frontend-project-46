{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
    	"no-console": 0,
    	"import/extensions": 0,
    	"no-undef": 0,
    	"import/no-named-as-default": 0,
    	"import/no-extraneous-dependencies": 0
    }
}
